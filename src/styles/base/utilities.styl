
.clearfix
  clearfix()

.pull-left
  float left

.pull-right
  float right

.table
  width 100%

.list-unstyled
  padding-left 0
  list-style none

.text-center
  text-align center
.text-left
  text-align left
.text-right
  text-align right
.text-truncate
  overflow hidden
  white-space nowrap
  text-overflow ellipsis

.bg-cover-center
  background-size cover
  background-repeat no-repeat
  background-position center center
